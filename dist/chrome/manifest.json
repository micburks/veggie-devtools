{
  "manifest_version": 2,

  "name": "Veggie Chrome Extension Dev Tool",
  "description": "Veggie devtools extension for affecting dev server responses",
  "version": "1.0",

  "minimum_chrome_version": "10.0",
  "devtools_page": "page.html",

  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/disabled-16.png",
      "32": "icons/disabled-32.png"
    }
  },

  "icons": {
    "16": "icons/enabled-16.png",
    "32": "icons/enabled-32.png"
  },

  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },

  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ]
}
